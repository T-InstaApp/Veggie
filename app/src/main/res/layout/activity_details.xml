<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/layout_background"
    android:orientation="vertical"
    android:padding="@dimen/_5sdp">

    <RelativeLayout
        android:id="@+id/layoutImage"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_170sdp">


        <ImageView
            android:id="@+id/menu_detail_back"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_25sdp"
            android:layout_margin="@dimen/_10sdp"
            android:src="@drawable/icon_back"
            app:tint="@color/new_theme_blue" />

        <TextView
            android:id="@+id/txt_unit_detail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_10sdp"
            android:background="@drawable/rect_button_background"
            android:padding="@dimen/_3sdp"
            android:text="$30.00"
            android:textColor="@color/white"
            android:textSize="@dimen/_16sdp"
            android:textStyle="bold" />

    </RelativeLayout>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardLayoutProductDesc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/layoutImage"
        android:layout_marginTop="@dimen/_5sdp"
        android:elevation="@dimen/_8sdp"
        app:cardBackgroundColor="@color/white"
        app:cardCornerRadius="@dimen/_5sdp">

        <androidx.appcompat.widget.LinearLayoutCompat
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/menu_detail_menu_name_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_8sdp"
                android:layout_marginTop="@dimen/_8sdp"
                android:text="Sweet corn soup"
                android:textColor="@color/heading_color"
                android:textSize="@dimen/_14sdp"
                android:textStyle="bold" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/menu_detail_menu_desc_text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_10sdp"
                android:text="Sweet corn soup jnbhk kjkk kh kjh hghghhghh   nhghghghg"
                android:textColor="@color/black"
                android:textSize="@dimen/_12sdp" />


        </androidx.appcompat.widget.LinearLayoutCompat>


    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/cardLayoutProductDesc"
        android:layout_marginTop="@dimen/_5sdp"
        android:layout_marginBottom="@dimen/_3sdp"
        android:elevation="@dimen/_8sdp"
        app:cardBackgroundColor="@color/white"
        app:cardCornerRadius="@dimen/_4sdp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_above="@+id/btn_add_to_cart"
                android:layout_marginTop="@dimen/_5sdp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <RelativeLayout
                        android:id="@+id/centerLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_8sdp"
                        android:visibility="visible">


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/_8sdp"
                            android:layout_marginRight="@dimen/_14sdp"
                            android:orientation="vertical">


                            <androidx.appcompat.widget.AppCompatTextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="@dimen/_8sdp"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:layout_marginRight="@dimen/_8sdp"
                                android:text="Quantity"
                                android:textColor="@color/heading_color"
                                android:textSize="@dimen/_14sdp" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_8sdp"
                                android:orientation="horizontal">


                                <androidx.appcompat.widget.LinearLayoutCompat
                                    android:layout_width="@dimen/_100sdp"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center_vertical"
                                    android:background="@drawable/circular_textfield_white"
                                    android:orientation="horizontal">

                                    <androidx.appcompat.widget.LinearLayoutCompat
                                        android:layout_width="@dimen/_25sdp"
                                        android:layout_height="match_parent"
                                        android:gravity="center_vertical">

                                        <androidx.appcompat.widget.AppCompatButton
                                            android:id="@+id/btn_minus"
                                            android:layout_width="match_parent"
                                            android:layout_height="@dimen/_20sdp"
                                            android:background="@drawable/ic_minus"
                                            android:backgroundTint="@color/black"
                                            android:drawableTint="@color/background" />
                                    </androidx.appcompat.widget.LinearLayoutCompat>


                                    <View
                                        android:layout_width="@dimen/_2sdp"
                                        android:layout_height="match_parent"
                                        android:background="@color/grey_40" />

                                    <TextView
                                        android:id="@+id/txt_dquntity"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:text="1"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_14sdp" />

                                    <View
                                        android:layout_width="@dimen/_2sdp"
                                        android:layout_height="match_parent"
                                        android:background="@color/grey_40" />

                                    <androidx.appcompat.widget.LinearLayoutCompat
                                        android:layout_width="@dimen/_25sdp"
                                        android:layout_height="match_parent"
                                        android:gravity="center_vertical">

                                        <androidx.appcompat.widget.AppCompatButton
                                            android:id="@+id/btn_add"
                                            android:layout_width="match_parent"
                                            android:layout_height="@dimen/_20sdp"
                                            android:background="@drawable/ic_add"
                                            android:backgroundTint="@color/black" />
                                    </androidx.appcompat.widget.LinearLayoutCompat>


                                </androidx.appcompat.widget.LinearLayoutCompat>

                                <!--Extra-->
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:visibility="gone">


                                    <TextView
                                        android:id="@+id/btn_minus1"
                                        android:layout_width="40dp"
                                        android:layout_height="40dp"
                                        android:layout_gravity="center"
                                        android:background="@drawable/ic_minus"
                                        android:gravity="center"
                                        android:text="-"
                                        android:textColor="@color/white"
                                        android:textSize="25sp"
                                        android:textStyle="bold" />

                                    <TextView
                                        android:id="@+id/txt_dquntity1"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="30dp"
                                        android:layout_marginRight="30dp"
                                        android:fontFamily="@font/nunito_bold"
                                        android:text="1"
                                        android:textSize="19sp"
                                        android:textStyle="bold" />

                                    <TextView
                                        android:id="@+id/btn_add1"
                                        android:layout_width="40dp"
                                        android:layout_height="40dp"
                                        android:background="@drawable/ic_add"
                                        android:gravity="center"
                                        android:text="+"
                                        android:textColor="@color/white"
                                        android:textSize="25sp"
                                        android:textStyle="bold" />
                                </LinearLayout>

                                <TextView
                                    android:id="@+id/txt_subtotaldetail"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="@dimen/_10sdp"
                                    android:gravity="center"
                                    android:text="$20.00"
                                    android:textColor="@android:color/black"
                                    android:textSize="@dimen/_19sdp"
                                    android:textStyle="bold" />

                            </LinearLayout>


                            <TextView
                                android:id="@+id/txt_ingredient"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:text="Size(Please select any option)"
                                android:textColor="@color/heading_color"
                                android:textSize="@dimen/_14sdp" />

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/rlc_size"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:visibility="visible" />

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/rlc_base"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:visibility="visible" />

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/rlc_pcontain"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:visibility="visible" />

                            <LinearLayout
                                android:id="@+id/lly_spin"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_5sdp"
                                android:layout_marginRight="10dp"
                                android:orientation="vertical"
                                android:visibility="gone">

                                <Spinner
                                    android:id="@+id/disspinner"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="@dimen/_5sdp"
                                    android:background="@drawable/gradient_spinner"
                                    android:inputType="textCapCharacters"
                                    android:spinnerMode="dropdown"
                                    android:textAllCaps="true" />

                            </LinearLayout>

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:text="Special Instructions"
                                android:textColor="@color/heading_color"
                                android:textSize="@dimen/_14sdp" />

                            <RelativeLayout
                                android:id="@+id/spcl_instruction"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/_3sdp"
                                android:layout_marginBottom="@dimen/_5sdp"
                                android:background="@drawable/rectangle_curved_green"
                                android:padding="@dimen/_5sdp">

                                <EditText
                                    android:id="@+id/edt_spcl_instruction"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@color/white"
                                    android:cursorVisible="true"
                                    android:gravity="left"
                                    android:maxLines="8"
                                    android:minLines="3"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_14sdp" />
                            </RelativeLayout>

                        </LinearLayout>


                        <TextView
                            android:id="@+id/product_namedetail"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_gravity="center"
                            android:layout_marginTop="@dimen/_40sdp"
                            android:text="Menu Tital Name"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/_20sdp"
                            android:visibility="gone" />
                    </RelativeLayout>


                </RelativeLayout>

            </androidx.core.widget.NestedScrollView>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btn_add_to_cart"
                android:layout_width="@dimen/_200sdp"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginBottom="@dimen/_10sdp"
                android:background="@drawable/rect_button_background"
                android:gravity="center"
                android:text="Add to cart"
                android:textColor="@color/white"
                android:textSize="@dimen/_10sdp" />
        </RelativeLayout>
    </androidx.cardview.widget.CardView>
